    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "electron": "^7.2.4",
    "electron-builder": "^20.43.0",
    "parcel-bundler": "^1.0.2"
  }
